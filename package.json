{
  "name": "devops-metrics-action",
  "version": "v1.0.0",
  "description": "TypeScript Action Template",
  "main": "out/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npx tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts --fix",
    "test": "jest --coverage",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/devops-metrics-action.git"
  },
  "keywords": [
    "DORA",
    "DevOps metrics"
  ],
  "author": "Sten Otto Johnsen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/actions/devops-metrics-action/issues"
  },
  "homepage": "https://github.com/actions/devops-metrics-action#readme",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@octokit/core": "^4.2.4",
    "@types/babel-core": "^6.25.7",
    "@types/json-query": "^2.2.3",
    "jshint": "^2.13.6",
    "json-query": "^2.2.2",
    "node-fetch": "^3.3.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.17",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@jest/globals": "^29.5.0",
    "@types/babel__core": "^7.20.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.3",
    "@types/node-fetch": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vercel/ncc": "^0.36.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-github": "^4.9.2",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.1",
    "js-yaml": "^4.1.0",
    "prettier": "3.0.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
